{{# partial "contenidoDeCreacion" }}

    <head>
        <link rel="stylesheet"  href="/indicadoresCreacion.css">
    </head>

    <style>

        .imagenes{
            font-size: 100px;
        }

        .imagenes span{
            margin-right: 5px;
            color: #2B75BE;
        }

    </style>

    <body>

    <div class="col-sm-2"></div>


    <div class="col-sm-8 parteIzq">

        <div class="contenidoCentral">

            <h2 class="centrar" style="padding-top: 60px; margin-bottom: 50px">Eliga una opcion: Indicador, Cuenta o Numero</h2>

            <div class="imagenes centrar">
                <span class="glyphicon glyphicon-arrow-down"></span>
                <span class="glyphicon glyphicon-arrow-down"></span>
                <span class="glyphicon glyphicon-arrow-down"></span>
            </div>

        </div>

        <div class="botonesICN">
            <form method="post">
                <input type="submit" name="cosaAOperar" value="Indicador"  class="botonSecundario"/>
                <input type="submit" name="cosaAOperar" value="Cuenta"  class="botonSecundario"/>
                <input type="submit" name="cosaAOperar" value="Numero"  class="botonSecundario"/>
            </form>
        </div>

    </div>

    <div class="col-sm-1 parteDer">

        <div class="botonesCalcu">
            <form>
                <div class="btnSuma">
                    <input type="submit" name="operador" value=" + " disabled class="botonSecundario"/>
                </div>
                <div class="btnResta">
                    <input type="submit" name="operador" value=" - " disabled class="botonSecundario"/>
                </div>
                <div class="btnMulti">
                    <input type="submit" name="operador" value=" * " disabled class="botonSecundario"/>
                </div>
                <div class="btnDiv">
                    <input type="submit" name="operador" value=" / " disabled class="botonSecundario"/>
                </div>

                <div class="btnCrear">
                    <input type="submit" name="operador" value="Crear" disabled class="botonPrincipal"/>
                </div>

            </form>
        </div>

    </div>


    <div class="col-sm-1"></div>


    </body>

{{/partial}}

